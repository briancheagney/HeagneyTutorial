





function oObject0_AnyClick(Object *theObject, CursorMode mode)
{
if (oButton.Graphic==280)
{
 int timer1limit=Random(2000)+1000;
 int timer2limit=Random(2000)+1000;
 int timer3limit=Random(2000)+1000;
 
 int spinnermove1=Random(10)+30;
 int spinnermove2=Random(5)+15;
 int spinnermove3=Random(5)+5;
 
 oButton.Graphic=279;
 int Timer=0;
 while (Timer<timer1limit)
 {
   oSpinner.Y=oSpinner.Y+spinnermove1;
   Timer=Timer+10;
   Wait(1);
   if (oSpinner.Y>2038)
   {
     oSpinner.Y=720;
   }
 }
 int Timer2=0;
 while (Timer2<timer2limit)
 {
  oSpinner.Y=oSpinner.Y+spinnermove2;
  Timer2=Timer2+40;
  Wait(1);
  if (oSpinner.Y>2038)
  {
    oSpinner.Y=720;
  }
 }
 int Timer3=0;
 while (Timer3<timer3limit)
 {
  oSpinner.Y=oSpinner.Y+spinnermove3;
  Timer3=Timer3+40;
  Wait(1);
  if (oSpinner.Y>2038)
  {
    oSpinner.Y=720;
  }
 }
 oButton.Graphic=280;
}
}

